\alias{gtkComboBoxRemoveText}
\name{gtkComboBoxRemoveText}
\title{gtkComboBoxRemoveText}
\description{Removes the string at \code{position} from \code{combo.box}. Note that you can only use
this function with combo boxes constructed with \code{\link{gtkComboBoxNewText}}.}
\usage{gtkComboBoxRemoveText(object, position)}
\arguments{
\item{\code{object}}{[\code{\link{GtkComboBox}}]  A \code{\link{GtkComboBox}} constructed with \code{\link{gtkComboBoxNewText}}.}
\item{\code{position}}{[integer]  Index of the item to remove.}
}
