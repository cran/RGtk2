\alias{gdkGCSetStipple}
\name{gdkGCSetStipple}
\title{gdkGCSetStipple}
\description{Set the stipple bitmap for a graphics context. The
stipple will only be used if the fill mode is
\code{GDK_STIPPLED} or \code{GDK_OPAQUE_STIPPLED}.}
\usage{gdkGCSetStipple(object, stipple)}
\arguments{
\item{\verb{object}}{[\verb{\link{GdkGC}}]  a \verb{\link{GdkGC}}.}
\item{\verb{stipple}}{[\verb{\link{GdkPixmap}}]  the new stipple bitmap.}
}
