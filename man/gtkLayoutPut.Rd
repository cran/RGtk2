\alias{gtkLayoutPut}
\name{gtkLayoutPut}
\title{gtkLayoutPut}
\description{Adds \code{child.widget} to \code{layout}, at position (\code{x},\code{y}).
\code{layout} becomes the new parent container of \code{child.widget}.}
\usage{gtkLayoutPut(object, child.widget, x, y)}
\arguments{
\item{\code{object}}{[\code{\link{GtkLayout}}]  a \code{\link{GtkLayout}}}
\item{\code{child.widget}}{[\code{\link{GtkWidget}}]  child widget}
\item{\code{x}}{[integer]  X position of child widget}
\item{\code{y}}{[integer]  Y position of child widget}
}
